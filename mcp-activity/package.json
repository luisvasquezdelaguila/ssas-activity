    "ssas",
    "activity",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.1",
    "axios": "^1.7.7",
    "dotenv": "^17.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0"
  },
  "bin": {
    "ssas-activity-mcp": "dist/index.js"
  }
}
